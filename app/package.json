{
  "name": "skytools-app",
  "private": true,
  "version": "1.4.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxt analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "pages:dev": "wrangler pages dev --port 3000 ./dist",
    "pages:build": "NITRO_PRESET=cloudflare-pages nuxt build",
    "pages:deploy": "NITRO_PRESET=cloudflare-pages nuxt build && wrangler pages deploy ./dist",
    "pages:preview": "wrangler pages dev ./dist --port 3000",
    "build:server": "mkdir -p dist/server && esbuild server/**/*.ts --bundle --minify --platform=node --target=node16 --outdir=dist/server --format=esm",
    "workers:dev": "wrangler dev --config ../wrangler.toml",
    "workers:build": "pnpm build:server",
    "workers:deploy": "pnpm workers:build && cd .. && wrangler deploy"
  },
  "workspaces": [
    "server"
  ],
  "devDependencies": {
    "@atproto/identity": "^0.4.2",
    "@nuxt/image": "^1.8.1",
